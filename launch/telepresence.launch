<launch>
	<include file="$(find rosbridge_server)/launch/rosbridge_websocket.launch" />
	<node name="web_video_server"
			pkg="web_video_server" type="web_video_server">
		<param name="port" value="8080"/>
		<param name="address" value="192.168.43.106"/>
	</node>

	<node name="telepresence" 
			pkg="porszilo_telepresence" type="flask_node_for_roslibjs_as_class.py" output="screen">
		<param name="frequency" value="10"/>
	</node>
</launch>
